{
  "name": "pairfly-core",
  "main": "src/index.js",
  "bin": "src/index.js",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "build": "pkg . --output dist/pairfly-core-exe",
    "build-webpack": "node --max-old-space-size=4096 node_modules/webpack/bin/webpack.js",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "jest -i --watchAll",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint ..",
    "lint:fix": "eslint .. --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js"
  },
  "dependencies": {
    "@appetism/binant-codetest-shared": "file:../shared",
    "amqplib": "^0.10.3",
    "bad-words": "^3.0.4",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "cross-env": "^7.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.0.0",
    "firebase-admin": "^11.0.0",
    "getenv": "^1.0.0",
    "helmet": "^4.1.0",
    "http-status": "^1.4.0",
    "ioredis": "^5.0.4",
    "joi": "^17.3.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "minio": "^7.0.33",
    "moment": "^2.29.4",
    "mongoose": "^5.13.16",
    "morgan": "^1.9.1",
    "multer": "^1.4.3",
    "node-fetch": "2",
    "nodemailer": "^6.3.1",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "socket.io": "^4.4.0",
    "socket.io-redis": "^6.1.1",
    "swagger-jsdoc": "^6.0.8",
    "swagger-ui-express": "^4.1.6",
    "twilio": "^4.9.0",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "winston": "^3.2.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.7",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^5.1.0",
    "helm-version-js": "^1.0.0",
    "jest": "^26.0.1",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "nodemon": "^2.0.0",
    "prettier": "^2.0.5"
  }
}
